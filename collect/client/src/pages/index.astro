---
import Layout from "../layouts/Layout.astro";

import fetchApi from "../lib/strapi";

const guitars = await fetchApi({
	endpoint: "guitars", // the content type to fetch
	wrappedByKey: "data", // the key to unwrap the response
});
---

<Layout>
	<p>Hello</p>
	{
		guitars.map((guitar) => (
			<li data-category={guitar?.slug}>
				<a href={`/guitar/${guitar.id}/`}>{guitar.name}</a>
			</li>
		))
	}
</Layout>
